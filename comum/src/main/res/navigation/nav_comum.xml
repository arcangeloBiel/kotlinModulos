<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_comum"
    app:startDestination="@id/loginFragment"
    tools:ignore="UnusedNavigation"
    >

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.comum.presentation.ui.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment_to_menuFragment"
            app:destination="@id/menuFragment" />
    </fragment>
    <fragment
        android:id="@+id/menuFragment"
        android:name="com.example.comum.presentation.ui.MenuFragment"
        android:label="menu_fragment"
        tools:layout="@layout/menu_fragment" >
        <action
            android:id="@+id/action_menuFragment_to_dispositivosFragment"
            app:destination="@id/dispositivosFragment" />
    </fragment>
    <fragment
        android:id="@+id/dispositivosFragment"
        android:name="com.example.comum.presentation.ui.DispositivosFragment"
        android:label="dispositivos_fragment"
        tools:layout="@layout/dispositivos_fragment" >
        <action
            android:id="@+id/action_dispositivosFragment_to_nav_picking2"
            app:destination="@id/nav_picking" />
        <action
            android:id="@+id/action_dispositivosFragment_to_nav_abastecimento"
            app:destination="@id/nav_abastecimento" />
        <argument
            android:name="menuName"
            app:argType="string" />
    </fragment>
    <include app:graph="@navigation/nav_picking" />
    <include app:graph="@navigation/nav_abastecimento" />
</navigation>